<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   
   <bean id="chassis" class="com.tapumandal.basic.Chassis">
   		<property name="frontWheelA" ref="wheel"/>
   		<property name="frontWheelB" ref="wheel"/>
   		<property name="rearWheelA" ref="wheel"/>
   		<property name="rearWheelB" ref="wheel"/>
   </bean>
   
   <bean id="wheel" class="com.tapumandal.basic.Wheel">
   		<property name="radious" value="1"/>
   		<property name="width" value="2"/>
   		<property name="company" value="VLOVO"/>
   		
   </bean>
   
</beans>